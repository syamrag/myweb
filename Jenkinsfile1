pipeline {
    agent any

    tools {
        maven 'maven'
    }

    stages {
        stage('Git Checkout') {
            steps {
                 git url: 'https://github.com/syamrag/myweb'
            }
        }
        stage('Example Deploy') {
            when {
                branch 'master'
            }
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
